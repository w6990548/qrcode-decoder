{
  "compilerOptions": {
    "outDir": "dist",
    "baseUrl": ".",
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "preserveConstEnums": true,
    "importHelpers": false,
    "lib": [
      "esnext",
      "dom"
    ],
    "module": "ESNext",
    "moduleResolution": "node",
    "sourceMap": false,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "strict": true,
    "target": "es5"
  }
}
